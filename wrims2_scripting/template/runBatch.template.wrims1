set wrims13_lib=%~dp0%lib\wrims13
set LookupDir={lookup_dir}
set runDir={run_dir}

set path=%runDir%\external;%wrims13_lib%;%path%

cd %runDir%

FOR %%A IN ( {year_range} ) DO (
	
	del  %runDir%\study.sty
    copy %runDir%\study_%%A.sty_PA  %runDir%\study.sty
	
	FOR %%B IN ( {table_list} ) DO (
		del  %LookupDir%\%%B.table
		copy %LookupDir%\%%B_%%A.table_PA  %LookupDir%\%%B.table
	)
	
echo Begin %%A run ...
%runDir%\{exe_name} %runDir%
echo End %%A run.
	
)


