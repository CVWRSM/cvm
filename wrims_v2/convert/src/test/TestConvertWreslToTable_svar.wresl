define DI_CVP_s {
    case MartoMay {
           condition  month >= MAR .and. month <= MAY 
           select     di
           from       wsi_di_cvp_s
           given      wsi=wsi_cvp_s 
           use        linear }      
                               
    case JuntoFeb { 
           condition  always 
           value      99.06 }                  
              }

define frcst_sac {
       case Febfore {
           condition  month == FEB
           select     FEB
           from       sacramento_runoff_forecast
           where      wateryear=wateryear }
       case JuntoJan {
           condition  always
           value      0  }
       }
define evap_1 {timeseries kind 'EVAPORATION-RATE' units 'IN'}
define evap_2 {timeseries kind 'EVAPORATION-RATE' units 'IN'}


