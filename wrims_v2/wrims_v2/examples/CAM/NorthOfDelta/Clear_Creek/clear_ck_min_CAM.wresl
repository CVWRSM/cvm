Sets minimum flow on Clear Creek below Whiskeytown for CAM.
*************************************/

define(CAMmonths) ClearCrk_min {
	select flow
        from   clear_ck_min
        where  month=mv($m),wytype=wyt_SHASTA_cvp($m)
}
                    	
define(CAMmonths) minflow_C_Wkytn {alias ClearCrk_min($m) kind 'flow-min-required' units 'cfs'}  

goal(CAMmonths) meet_clearmin {C_Wkytn_MIF($m) < ClearCrk_min($m)}  
