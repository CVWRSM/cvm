define(CAMmonths) C_Orovl {lower 0. upper 12913.*taf_cfs($m) kind 'FLOW-CHANNEL' units 'CFS'} !FEATHER RIVER DOWNSTREAM OF OROVILLE
define(CAMmonths) minflow_C_Orovl {
			case beforeSep{
				condition month+$m<=12
				select minflow from minflow where C_arc=200.1,month=month+$m} !use values in the range of 700-800 to replace 600 cfs
			case afterSep{
				condition month+$m>12
				select minflow from minflow where C_arc=200.1,month=month+$m-12} !use values in the range of 700-800 to replace 600 cfs	
		}
define(CAMmonths) C_Orovl_MIF {std kind 'FLOW-MIN-INSTREAM' units 'CFS'}
define(CAMmonths) C_Orovl_EXC {std kind 'FLOW-EXCESS-INSTREAM' units 'CFS'}
goal(CAMmonths) C_Orovltotal {C_Orovl($m)=C_Orovl_MIF($m)+C_Orovl_EXC($m)}
goal(CAMmonths) C_Orovlminflow {C_Orovl_MIF($m) < minflow_C_Orovl($m)}
